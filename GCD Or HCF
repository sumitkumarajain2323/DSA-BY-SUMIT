#include <iostream>
using namespace std;

int main() 
{
  int n1= 15;
  int n2 = 5;
    
    if(n1<n2){
            for(int i=n1;i>1;i--){
                if( ((n1%i)==0) && ((n2%i)==0)){
                    cout<< i;
                    return 0;
                }
            }
            return 1;

        }
        else{
            for(int i=n2;i>1;i--){
                if(((n2%i)==0) && ((n1%i)==0)){
                    cout<< i;
                    return 0;
                }
            }
            return 1;
        }
    
    return 0;
}
